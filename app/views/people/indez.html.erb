<h1>People#indez</h1>
<p>Find me in app/views/people/indez.html.erb</p>

<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>email</th>
      <th>age</th>
      <th>Detalle</th>
      <th>Editar</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.id %></td>
        <td><%= person.name %></td>
        <td><%= person.email %></td>
        <td><%= person.edad %></td>
        <td><%= link_to "Ver", people_show_path(person.id)%></td>
         <td><%= link_to "Editar", people_edit_path(person.id)%></td>
      </tr>
    <%end%>
  </tbody>
</table>